}

@article{DeLind2011,
  title = {Are Local Food and the Local Food Movement Taking Us Where We Want to Go? {{Or}} Are We Hitching Our Wagons to the Wrong Stars?},
  volume = {28},
  issn = {0889-048X, 1572-8366},
  shorttitle = {Are Local Food and the Local Food Movement Taking Us Where We Want to Go?},
  abstract = {Much is being made of local food. It is at once a social movement, a diet, and an economic strategy\textemdash{}a popular solution\textemdash{}to a global food system in great distress. Yet, despite its popularity or perhaps because of it, local food (especially in the US) is also something of a chimera if not a tool of the status quo. This paper reflects on and contrasts aspects of current local food rhetoric with Dalhberg's notion of a regenerative food system. It identifies three problematic emphases\textemdash{}the locavore emphasis, the Wal-Mart emphasis, and the Pollan emphasis\textemdash{}and argues that they are shifting local food (as a concept and a social movement) away from the deeper concerns of equity, citizenship, place-building, and sustainability. It is suggested that local food activists and advocates might consider the use of multiple methodologies and forms of expression to explore the integration and reintegration of local food into diverse and redundant place-based practice. A short case study of a low-income, urban neighborhood in Lansing, Michigan, illustrates the value of contextual analysis for more fully enabling the local food movement and a regenerative food system.},
  language = {en},
  number = {2},
  journal = {Agriculture and Human Values},
  doi = {10.1007/s10460-010-9263-0},
  author = {DeLind, Laura B.},
  month = jun,
  year = {2011},
  pages = {273-283},
  file = {F:\\Library\\Zotero Data\\storage\\H6CFDWL2\\DeLind - 2011 - Are local food and the local food movement taking .pdf}
}

