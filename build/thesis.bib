  keywords = {differentiation,proliferation,brain development,epigenetics,H2B ubiquitylation,neural stem cells},
  pages = {3067-3078},
  file = {F:\\Library\\Ishino et al_2014_Bre1a, a Histone H2B Ubiquitin Ligase, Regulates the Cell Cycle and.pdf;F:\\Library\\Zotero Data\\storage\\9ZQL32MV\\3067.html},
