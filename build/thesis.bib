  journal = {Cell Death \& Disease},
  doi = {10.1038/s41419-018-0411-6},
  author = {Zhou, Yi and Wei, Ran and Zhang, Liu and Chen, Yongjiang and Lu, Suying and Liang, Chen and Wang, Yujiao and Xiao, Lirong and Zhang, Junjun and Bremner, Rod and Chen, Danian},
  month = mar,
  year = {2018},
  file = {F:\\Library\\Zotero Data\\storage\\Y36FPX9Z\\Zhou et al. - 2018 - Rb is required for retinal angiogenesis and lamina.pdf}
}

