  month = mar,
  year = {2017},
  keywords = {China,Earth and environmental sciences,Environmental sciences,natural resources and environmental carrying index,regional development sustainability,risk forewarning,Sustainable development,theoretical model},
  pages = {196-213},
  file = {F:\\Library\\Fan Jie et al_2017_Risk forewarning of regional development sustainability based on a natural.pdf;F:\\Library\\Zotero Data\\storage\\IA2KI6S6\\2016EF000490.html}
