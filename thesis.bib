@article{Zhou2008,
  title = {On the Use of an {{SPSA}}-Based Model-Free Feedback Controller in Active Noise Control for Periodic Disturbances in a Duct},
  author = {Zhou, Ya-Li and Zhang, Qi-Zhi and Li, Xiao-Dong and Gan, Woon-Seng},
  year = {2008},
  month = nov,
  volume = {317},
