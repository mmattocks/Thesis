}

@article{Feldt2010,
  title = {Governing {{Through}} the {{Dao}}: {{A Non}}-{{Anarchistic Interpretation}} of the {{Laozi}}},
  shorttitle = {Governing {{Through}} the {{Dao}}},
  author = {Feldt, Alex},
  year = {2010},
  month = sep,
  volume = {9},
  pages = {323--337},
  issn = {1540-3009, 1569-7274},
  doi = {10.1007/s11712-010-9176-z},
  abstract = {Within the literature, Daoist political philosophy has often been linked with anarchism. While some extended arguments have been offered in favor of this conclusion, I take this position to be tenuous and predicated on an assumption that coercive authority cannot be applied through wuwei. Focusing on the Laozi as the fundamental political text of classical Daoism, I lay out a general account of why one ought to be skeptical of classifying it as anarchistic. Keeping this skepticism in mind and recognizing the importance of wuwei in arguments for the anarchist conclusion, I provide a non-anarchistic interpretation of wuwei as a political technique that is consistent with the text of the Laozi. Having presented a plausible alternative to the anarchist understanding of wuwei, I close my discussion with a brief sketch of a positive account of the political theory of the Laozi.},
  file = {/bench/Library/Feldt_2010_Governing Through the Dao.pdf},
  journal = {Dao},
  language = {en},
  number = {3}
}

@article{Felzenszwalb,
