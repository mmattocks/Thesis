  file = {/bench/Library/Zotero Data/storage/J5EVRPDZ/Denton et al. - 2019 - Eigenvectors from Eigenvalues a survey of a basic.pdf;/bench/Library/Zotero Data/storage/ZCB6D8AD/soler-toscano2014.pdf},
  journal = {arXiv:1908.03795 [hep-th, physics:math-ph]},
  keywords = {High Energy Physics - Theory,Mathematical Physics,Mathematics - Rings and Algebras},
  language = {en},
