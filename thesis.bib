@article{Nubold2017,
  title = {Developing a {{Taxonomy}} of {{Dark Triad Triggers}} at {{Work}} \textendash{} {{A Grounded Theory Study Protocol}}},
  author = {N{\"u}bold, Annika and Bader, Josef and Bozin, Nera and Depala, Romil and Eidast, Helena and Johannessen, Elisabeth A. and Prinz, Gerhard},
