@article{Almeida2018,
  title = {Chromatin Conformation Regulates the Coordination between {{DNA}} Replication and Transcription},
  author = {Almeida, Ricardo and {Fern{\'a}ndez-Justel}, Jos{\'e} Miguel and {Santa-Mar{\'i}a}, Cristina and Cadoret, Jean-Charles and {Cano-Aroca}, Laura and Lombra{\~n}a, Rodrigo and Herranz, Gonzalo and Agresti, Alessandra and G{\'o}mez, Mar{\'i}a},
