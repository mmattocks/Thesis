  file = {/bench/Library/Zotero Data/storage/5L6LE5ZB/index.html},
  journal = {Wikipedia},
  language = {en}
}

@article{Looney2018,
  title = {Metabolic {{Costs}} of {{Military Load Carriage}} over {{Complex Terrain}}},
  author = {Looney, David P and Santee, William R and Karis, Anthony J and Blanchard, Laurie A and Rome, Maxwell N and Carter, Alyssa J and Potter, Adam W},
  year = {2018},
  month = sep,
  volume = {183},
  pages = {e357-e362},
  issn = {0026-4075, 1930-613X},
  doi = {10.1093/milmed/usx099},
  abstract = {Introduction: Dismounted military operations often involve prolonged load carriage over complex terrain, which can result in excessive metabolic costs that can directly impair soldiers' performance. Although estimating these demands is a critical interest for mission planning purposes, it is unclear whether existing estimation equations developed from controlled laboratory- and field-based studies accurately account for energy costs of traveling over complex terrain. This study investigated the accuracy of the following equations for military populations when applied to data collected over complex terrain with two different levels of load carriage: American College of Sports Medicine (2002), Givoni and Goldman (1971), Jobe and White (2009), Minetti et al (2002), Pandolf et al (1977), and Santee et al (2003). Materials and Methods: Nine active duty military personnel (age 21 {$\pm$} 3 yr; height 1.72 {$\pm$} 0.07 m; body mass 83.4 {$\pm$} 12.9 kg; VO2 max 47.8 {$\pm$} 3.9 mL/kg/min) were monitored during load carriage (with loads equal to 30\% and 45\% of body mass) over a 10-km mixed terrain course on two separate test days. The course was divided into four 2.5-km laps of 40 segments based on distance, grade, and/or surface factors. Timing gates and radio-frequency identification cards (SportIdent; Scarborough Orienteering, Huntington Beach, CA) were used to record completion times for each course segment. Breath-by-breath measures of energy expenditure were collected using portable oxygen exchange devices (COSMED Sri., Rome, Italy) and compared model estimates. Results: The Santee et al equation performed best, demonstrating the smallest estimation bias (-13 {$\pm$} 87 W) and lowest root mean square error (99 W). Conclusion: Current predictive equations underestimate the metabolic cost of load carriage by military personnel over complex terrain. Applying the Santee et al correction factor to the Pandolf et al equation may be the most suitable approach for estimating metabolic demands in these circumstances. However, this work also outlines the need for improvements to these methods, new method development and validation, or the use of a multi-model approach to account for mixed terrain.},
  file = {/bench/Library/Zotero Data/storage/YLTWI3RF/Looney et al. - 2018 - Metabolic Costs of Military Load Carriage over Com.pdf},
  journal = {Military Medicine},
  language = {en},
  number = {9-10}
}

