@article{Icha2016,
  title = {Independent Modes of Ganglion Cell Translocation Ensure Correct Lamination of the Zebrafish Retina},
  author = {Icha, Jaroslav and Kunath, Christiane and {Rocha-Martins}, Mauricio and Norden, Caren},
  year = {2016},
